alias gp="git push"
alias gpl="git pull"
alias gd="git diff"
alias gs="git status"
alias gl="git log"
alias gb="git branch"
alias gcb="git checkout -b"
alias gch="git checkout"
alias gchh="git checkout master"
alias gchhi="git checkout main"
alias ga="git add"
alias gc="git commit -m"
alias gca="git commit -am"
alias gcam="git commit --amend"
alias gcp="git cherry-pick"
alias gcpc="git cherry-pick --continue"
alias gcpa="git cherry-pick --abort"
alias grb="git rebase"
alias grbc="git rebase --continue"
alias grba="git rebase --abort"
alias gret="git reset"
alias grets="git reset --soft"
alias greth="git reset --hard"
alias grto="git restore"
alias grtos="git restore --staged"
alias gmff="git merge --ff-only"
alias gfom="git fetch origin master"
alias gmffom="gmff origin master"
alias gfomi="git fetch origin main"
alias gmffomi="gmff origin main"
alias gum="gfom && gmffom"
alias gumi="gfomi && gmffomi"
alias gf="git fetch"
alias gm="git merge"
alias grl="git reflog"
